		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Total Discounts by Country' = CALCULATE(SUM(Financials[Discounts]), ALLEXCEPT(Financials, Financials[Country]))
		lineageTag: cc5db508-697c-4d9f-a1e3-e8d63e49472f

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Discount Percentage' = ```
			DIVIDE([Total Discounts by Country], [Total Sales by Country], 0)
			
			```
		formatString: #,0.00%;-#,0.00%;#,0.00%
		lineageTag: 7f88a628-9e64-4e6c-8d00-c681d2722e5e

