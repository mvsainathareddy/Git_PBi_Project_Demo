		lineageTag: 3ec90781-a510-49ab-b3b5-0dde5c84c612

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Gross Profit' = [Total Sales] -  [Total COGS]
		displayFolder: Sales
