	measure 'Total Sales US' =
			--CALCULATE(SUM(financials[ Sales]), financials[Country] = "United States of America")
			 CALCULATE(sum(financials[Total RV*]), financials[Country] = "United States of America")
		lineageTag: 6ce7e4b4-b555-46df-881c-6aa89e4f119c

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Total Revenue US' =
			CALCULATE(
			    SUMX(financials, financials[Sale Price] * financials[Units Sold]),
			    financials[Country] = "United States of America"
			)
			//CALCULATE modifies the filter context in which a DAX expression is evaluated
		formatString: 0
		lineageTag: e2bdf67e-b5b2-46b1-a2f0-c82bd051b33a

	measure 'Total Sales' =
			SUM(financials[ Sales])
			//Where to Use: Use these measures in a sales performance dashboard to track overall sales and profitability.
			//Why: These measures provide a quick overview of the company's financial performance, helping stakeholders make informed decisions.
			//Importance:DAX measures allow for dynamic calculations that update automatically with data changes, ensuring accurate and up-to-date insights.
		displayFolder: Sales
		lineageTag: 6bbddc50-03a5-4a44-8a5d-d078cb4dd03d

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Total COGS' = SUM(Financials[COGS])
