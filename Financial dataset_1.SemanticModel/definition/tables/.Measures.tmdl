		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Max Country Sale2' =
			SUMX(ADDCOLUMNS(SUMMARIZE( 'Dim Date','Dim Date'[Year],'Dim Date'[Quarter])
