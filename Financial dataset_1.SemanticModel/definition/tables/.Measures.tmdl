		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Net Sales' = [Total Sales] - [Total Discounts]
		formatString: \$#,0.00;(\$#,0.00);\$#,0.00
		displayFolder: Sales
		lineageTag: 286ad2ff-f3f2-4659-b66c-e2773f0b1aae

	measure 'Profit Margin' = DIVIDE([Gross Profit], [Total Sales], 0)
		formatString: 0.00%;-0.00%;0.00%
		displayFolder: Sales
		lineageTag: d4a3c1ec-c2f3-4f56-a6a6-eac27c66a34d

	measure 'Total Revenue sumx' = ```
			//multiplies Sales Price by Units Sold for each row and then sums the results
			 SUMX(
			    Financials, -- 1.DAX returns the sales by financials
			    Financials[Sale Price] * Financials[Units Sold]  -- 2.and then eavalutes the expression 
			    )
			```
		formatString: 0
		lineageTag: ffa937a6-dcad-42fc-8dd3-0c0121fcdf81

	measure 'Sales Category' = IF(Financials[ Sales] > 1000, "High", "Low")  --- measure or cal?
		lineageTag: 89fd2972-2d77-497a-878e-fa8653db9448

	measure 'Discount Band Measure' =
			SWITCH(
			    TRUE(),
			    [Discount Percentage] > 0.08, "High",  -- More than 10%
			    [Discount Percentage] > 0.07, "Medium",  -- More than 5%
			    [Discount Percentage] > 0.01, "Low",  // More than 0%
			    "None"
			)
		lineageTag: d0f6b290-32fc-47cf-be4f-8d9499671290

	measure 'Total Sales by Country' = CALCULATE(SUM(Financials[ Sales]), ALLEXCEPT(Financials, Financials[Country]))
